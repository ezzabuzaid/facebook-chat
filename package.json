{
  "scripts": {
    "ng": "ng",
    "e2e": "ng e2e",
    "start": "ng serve",
    "start:ssr": "node dist/server",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint './src/**/*.scss' --fix",
    "build": "ng build --prod",
    "build:ssr": "npm run build:app-shell && npm run compile:server",
    "build:app-shell": "ng run buildozer:app-shell:production",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "extract": "ngx-translate-extract  -i ./src -o ./src/assets/languages/ar.json ./src/assets/languages/en.json --sort --clean --marker _extract",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "deploy": "ng build --prod --base-href 'https://ezzabuzaid.github.io/angular-buildozer/'",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@angular/cdk": "^8.1.3",
    "@angular/fire": "^5.2.1",
    "@angular/material": "^8.1.3",
    "@angular/platform-server": "~8.2.3",
    "@angular/pwa": "^0.803.0",
    "@angular/service-worker": "~8.2.3",
    "@auth0/angular-jwt": "^3.0.0",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@types/bootstrap": "^4.3.1",
    "@types/express": "^4.17.1",
    "@types/firebase": "^3.2.1",
    "@types/hammerjs": "^2.0.36",
    "@wapps/jest-google-maps-mock": "0.0.9",
    "bootstrap": "^4.3.1",
    "express": "^4.15.2",
    "firebase": "^6.4.2",
    "hammerjs": "^2.0.8",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-perfect-scrollbar": "^8.0.0",
    "semi-table": "0.0.5",
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@biesbjerg/ngx-translate-extract": "^3.0.3",
    "@types/googlemaps": "^3.37.4",
    "@types/stylelint": "^9.10.1",
    "husky": "^3.0.4",
    "mock-browser": "^0.92.14",
    "stylelint": "^10.1.0",
    "ts-loader": "^6.0.4",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run build:ssr && git add .",
      "pre-push": "npm run build:ssr"
    }
  },
  "name": "angular-buildozer",
