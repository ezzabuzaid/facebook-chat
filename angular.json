{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "buildozer": {
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "spec": true
        },
        "@schematics/angular:module": {
          "routing": true,
          "routingScope": "Child"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/intl-tel-input/build/css/intlTelInput.min.css"
            ],
            "scripts": [
              "node_modules/intl-tel-input/build/js/intlTelInput.min.js",
              "node_modules/intl-tel-input/build/js/utils.js",
              "node_modules/simple-peer/simplepeer.min.js"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "15mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "100kb"
                }
              ],
              "statsJson": true,
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "fileReplacements": [
              {
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }
            ],
            "outputHashing": "media",
            "sourceMap": false,
            "optimization": false,
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss"
              ]
            }
          },
          "configurations": {
            "production": {}
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "buildozer:build:production",
            "serverTarget": "buildozer:server:production",
            "routes": [
              "/portal"
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "buildozer:build",
            "serverTarget": "buildozer:server"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "buildozer:build",
            "aot": true
          },
          "configurations": {
            "production": {
              "browserTarget": "buildozer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "buildozer:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss"
              ]
            }
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "buildozer:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "buildozer:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "buildozer",
  "cli": {
    "analytics": "c0863d07-34f4-4b95-b876-0ab1036d2c37"
  }
}